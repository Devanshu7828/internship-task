
<div class="container table-container">
  <%- include('./partials/_header.ejs')%>
  <table class="table table-dark">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">Name</th>
        <th scope="col">Last</th>
        <th scope="col">Buy/Sell</th>
        <th scope="col">Volume</th>
        <th scope="col">Base_unit</th>
      </tr>
    </thead>
    <tbody class="body">
      <% for(let i=0;i<10;i++){%>
      <tr>
        <th scope="row"><%=i+1%></th>
        <td><%=stock[i].name%></td>
        <td>₹ <%=stock[i].last%></td>
        <td>₹ <%=stock[i].buy%>/<%=stock[i].sell%></td>
        <td>₹ <%=stock[i].volume%></td>
        <td> <%=stock[i].base_unit%></td>
      </tr>
      <%}%>
    </tbody>
  </table>
</div>
